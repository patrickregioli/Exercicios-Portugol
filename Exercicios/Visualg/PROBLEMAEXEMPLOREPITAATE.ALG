Algoritmo "problemaexemplorepitaate"

Var
   c, f : real
   i : caractere


Inicio

   repita
      escreva("Digite a temperatura em Celsius: ")
      leia(c)
      f <- c * 1.8 + 32
      escreval("Temperatura equivalente em Fahrenheit: ", f:4:1)
      escreva("Deseja repetir?:(s/n) ")
      leia(i)
   ate i <> "s"

   




Fimalgoritmo